#include<stdio.h>
#include<stdlib.h>

int main()
{
    int h1,h2;
    float w1,w2,m;
    printf("Enter height(-1 to exit) :");
    scanf("%d %d",&h1,&h2);
    for(int i=h1;i<=h2;i++)
    {
        m=i/100.0;
        w1=18.5*m*m;
        w2=24.0*m*m;
        printf("%d cm: %.1f ~ %.1f (kg)\n",i,w1,w2);
    }
    return 0;
}
